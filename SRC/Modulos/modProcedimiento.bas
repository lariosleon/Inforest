Public Sub ValidaStr(Cad As TextBox)
  Dim sCad As String
  Dim nRes As Integer
  nRes = 0
  sCad = Trim(Cad.Text)
  nRes = InStr(1, sCad, "&")
  If nRes > 0 Then
     MsgBox "Caracter (&) no permitido, favor corregir", vbExclamation, sMensaje
     Cad.SelStart = nRes - 1
     Cad.SetFocus
     Exit Sub
  End If
  
  nRes = InStr(1, sCad, "'")
  If nRes > 0 Then
     MsgBox "Caracter (') no permitido, favor corregir", vbExclamation, sMensaje
     Cad.SelStart = nRes - 1
     Cad.SetFocus
     Exit Sub
  End If
  
  nRes = InStr(1, sCad, """")
  If nRes > 0 Then
     MsgBox "Caracter ("") no permitido, favor corregir", vbExclamation, sMensaje
     Cad.SelStart = nRes - 1
     Cad.SetFocus
     Exit Sub
  End If
End Sub

