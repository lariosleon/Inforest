Public Function getFilter(cols As TrueOleDBGrid80.Columns) As String
    Dim tmp As String
    Dim n As Integer
    
    For Each col In cols
        If Trim(col.FilterText) <> "" Then
            n = n + 1
            If n > 1 Then
                tmp = tmp & " AND "
            End If
            If col.DataField = "lactivo" Then
                tmp = tmp & col.DataField & " = '" & col.FilterText & "'"
