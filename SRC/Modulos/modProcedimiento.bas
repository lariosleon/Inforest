   If RsModulo.RecordCount > 0 Then
      For Each Control In formulario.Controls
          RsModulo.MoveFirst
          RsModulo.Find ("tObjeto='" & Control.Name & "'")
          If Not RsModulo.EOF Then
             Control.Enabled = False
          End If
      Next Control
   End If
   Set RsModulo = Nothing
End Sub

