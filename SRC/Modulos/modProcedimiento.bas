        End If
        
        ImprimeXLinea "Obs.: " & IIf(IsNull(RsImpresion!Observacion), "", Trim(RsImpresion!Observacion)), 37, 6
        
    
        '----------------------------------------------------------------------------------------------------------
        '--------------------------------------Fin de Cabecera-----------------------------------------------------
