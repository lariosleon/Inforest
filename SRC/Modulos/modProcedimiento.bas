                       Printer.Print "Personas: " & RsImpresion!nAdulto '& " - " & IIf(RsImpresion!Prioridad, "Urgente", "Regular")
                    End If
                    
               Case Is = "02"
                    xPedidox = sPedido
                    Printer.Print "Motoriz.: " & frmVenta.txtMotorizado.Caption
