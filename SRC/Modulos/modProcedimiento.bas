   For i = 0 To nCampo - 1
       'Isql = Isql & Lista(i * 2) & " " & Lista((i * 2) + 1) & ", "
       If UCase(Left(LTrim(Lista((i * 2) + 1)), 8)) = "NVARCHAR" Or UCase(Left(LTrim(Lista((i * 2) + 1)), 7)) = "VARCHAR" Then
