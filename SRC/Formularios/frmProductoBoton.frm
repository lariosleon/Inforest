Attribute VB_Name = "frmProductoBoton"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit
Dim RsBoton As Recordset
Dim RsCajaRapida As Recordset
Dim sPrefijo As String
Dim Color As String
Dim i As Integer
Dim nBotonAnterior As Integer
Dim sCajaRapida As String

Private Sub cmdAgrupacion_Click(Index As Integer)
   For i = 1 To 42
       cmdBoton(i).BackColor = cmdAgrupacion(Index).BackColor
       cmdBoton(i).Caption = i
       cmdBoton(i).Enabled = True
   Next i
   RsBoton.Filter = "tCajaRapida=" & sPrefijo & Index
   If Not RsBoton.EOF Then
      RsBoton.MoveFirst
      Do While Not RsBoton.EOF
         If RsBoton!nBotonRapido > 0 Then
            cmdBoton(RsBoton!nBotonRapido).Enabled = False
            cmdBoton(RsBoton!nBotonRapido).Caption = RsBoton!tResumido
         End If
         RsBoton.MoveNext
      Loop
   End If
   sCajaRapida = sPrefijo & Index
End Sub

Private Sub cmdBoton_Click(Index As Integer)
   If nBotonAnterior <> 0 Then
      cmdBoton(nBotonAnterior).Enabled = True
      cmdBoton(nBotonAnterior).Caption = Trim(nBotonAnterior)
   End If
   cmdBoton(Index).Enabled = False
   cmdBoton(Index).Caption = frmProductoDetalle.txtResumido.Text
   nBotonAnterior = Index
End Sub

Private Sub cmdEtiqueta_Click(Index As Integer)
    sPrefijo = Index
    RsCajaRapida.Filter = "Prefijo='" & sPrefijo & "'"
    RsCajaRapida.MoveFirst
    cmdEtiqueta(1).FontBold = IIf(Index = 1, True, False)
    cmdEtiqueta(2).FontBold = IIf(Index = 2, True, False)
    cmdEtiqueta(3).FontBold = IIf(Index = 3, True, False)
    
    For i = 1 To 9
        cmdAgrupacion(i).BackColor = IIf(RsCajaRapida!nValor = 0, -2147483633, RsCajaRapida!nValor)
        If LTrim(RsCajaRapida!tDetallado) = "" Then
           cmdAgrupacion(i).Caption = "(no utilizado)"
           cmdAgrupacion(i).Enabled = False
        Else
           cmdAgrupacion(i).Caption = RsCajaRapida!tDetallado
           cmdAgrupacion(i).Enabled = True
        End If
        RsCajaRapida.MoveNext
    Next i
End Sub

Private Sub cmdOpcion_Click(Index As Integer)
   Select Case Index
      Case Is = 1   'Cancelar
           Unload Me

      Case Is = 2   'Grabar
           Isql = "update TPRODUCTO set " & _
                  "nBotonRapido =" & nBotonAnterior & ", " & _
                  "tCajaRapida ='" & sCajaRapida & "' " & _
                  ",lreplica=1 where tCodigoProducto = '" & frmProductoDetalle.txtCodigo.Text & "'"
           Cn.Execute Isql
           RsBoton.Requery
           Unload Me
   End Select
End Sub

Private Sub cmdSinBoton_Click()
   If nBotonAnterior <> 0 Then
      cmdBoton(nBotonAnterior).Enabled = True
      cmdBoton(nBotonAnterior).Caption = nBotonAnterior
      nBotonAnterior = 0
   End If
End Sub

Private Sub Form_Load()
    Centrar Me
    nBotonAnterior = 0
    cmdEtiqueta(1).Caption = Calcular("select tDetallado as Codigo from TTABLA where tTabla='ETIQUETA' and tCodigo='01'", Cn)
    cmdEtiqueta(2).Caption = Calcular("select tDetallado as Codigo from TTABLA where tTabla='ETIQUETA' and tCodigo='02'", Cn)
    cmdEtiqueta(3).Caption = Calcular("select tDetallado as Codigo from TTABLA where tTabla='ETIQUETA' and tCodigo='03'", Cn)
    
    cmdEtiqueta(1).Caption = IIf(cmdEtiqueta(1).Caption = "0", "", cmdEtiqueta(1).Caption)
    cmdEtiqueta(2).Caption = IIf(cmdEtiqueta(1).Caption = "0", "", cmdEtiqueta(2).Caption)
    cmdEtiqueta(3).Caption = IIf(cmdEtiqueta(1).Caption = "0", "", cmdEtiqueta(3).Caption)
    
    Set RsCajaRapida = Lib.OpenRecordset("select tCodigo, substring(tCodigo,1,1) as Prefijo, tDetallado, nValor from TTABLA where tTabla='CAJARAPIDA'", Cn)
    Set RsBoton = Lib.OpenRecordset("select tCodigoProducto as Codigo, tResumido, substring(tCajaRapida,1,1) as Prefijo, tCajaRapida, nBotonRapido from TPRODUCTO where lActivo=1", Cn)
    Asignar
End Sub

Public Sub Asignar()
   RsBoton.Find ("Codigo='" & frmProductoDetalle.txtCodigo.Text & "'")
   If RsBoton.EOF Then
      RsBoton.MoveFirst
      cmdEtiqueta_Click (1)
      cmdAgrupacion_Click (1)
   Else
      If IsNull(RsBoton!Prefijo) Or RsBoton!Prefijo = "" Then
         cmdEtiqueta_Click (1)
         cmdAgrupacion_Click (1)
      Else
         cmdEtiqueta_Click (RsBoton!Prefijo)
         nBotonAnterior = RsBoton!nBotonRapido
         cmdAgrupacion_Click Mid(RsBoton!tcajarapida, 2, 1)
      End If
   End If
End Sub
