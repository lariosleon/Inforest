Public Function obtieneAnoMes(Ruta As String) As String
        Open Ruta For Input As #1
        Dim linea As String, TOTAL As String
        Dim a, b, c, d, e, f As String
        Do Until EOF(1)
        Line Input #1, linea
        TOTAL = TOTAL + linea
        Loop
        Close #1
        a = Mid(TOTAL, 17, 1)
        b = Mid(TOTAL, 6, 1)
        c = Mid(TOTAL, 30, 1)
        d = Mid(TOTAL, 14, 1)
        e = Mid(TOTAL, 8, 1)
        f = Mid(TOTAL, 25, 1)
        obtieneAnoMes = c & d & e & f & a & b
End Function

